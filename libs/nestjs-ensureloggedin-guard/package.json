{
  "name": "nestjs-ensureloggedin-guard",
  "version": "1.1.0",
  "description": "NestJS Ensure Loggedin Guard",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack || echo not ok",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "release": "standard-version && npm pack",
    "format": "prettier --write \"./src/**/*.ts\"",
    "lint": "tslint -c tslint.json './src/**/*.ts'"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "browserify": "^16.2.3",
    "delete-empty": "^2.0.0",
    "dts-bundle": "^0.7.3",
    "jest": "^23.2.0",
    "prettier": "^1.11.1",
    "standard-version": "^4.2.0",
    "ts-jest": "^23.0.0",
    "ts-loader": "^5.3.2",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/passport": "^5.1.0",
    "passport": "^0.4.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/",
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "testRegex": "__tests__/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
