{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "authorization-server": {
      "root": "",
      "sourceRoot": "src/client",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/authorization-server",
            "index": "src/client/index.html",
            "main": "src/client/main.ts",
            "polyfills": "src/client/polyfills.ts",
            "tsConfig": "src/client/tsconfig.app.json",
            "assets": [
              "src/client/favicon.ico",
              "src/client/assets"
            ],
            "styles": [
              "src/client/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "mr": {
              "aot": true,
              "baseHref": "/language/mr/",
              "outputPath": "dist/authorization-server-mr/",
              "i18nFile": "src/client/i18n/messages.mr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "mr",
              "i18nMissingTranslation": "error"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server",
            "main": "src/client/main.server.ts",
            "tsConfig": "src/client/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }

        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "authorization-server:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "authorization-server:build:production"
            },
            "ci": {
              "progress": false
            },
            "mr": {
              "browserTarget": "authorization-server:build:mr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "authorization-server:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/client/test.ts",
            "polyfills": "src/client/polyfills.ts",
            "tsConfig": "src/client/tsconfig.spec.json",
            "karmaConfig": "src/client/karma.conf.js",
            "styles": [
              "src/client/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/client/favicon.ico",
              "src/client/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/client/tsconfig.app.json",
              "src/client/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "authorization-server-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "authorization-server:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "authorization-server:serve:production"
            },
            "ci": {
              "devServerTarget": "authorization-server:serve:ci"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "authorization-server"
}